{
  "expo": {
    "name": "Storyblok AI APP",
    "slug": "Storyblok-AI-APP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aiapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.AIAPP",
      "config": {
        "googleSignIn": {
          "reservedClientId": "384183779621-hasnghd0v1ghcbqikl3f5oln7c20jeat.apps.googleusercontent.com"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.AIAPP",
      "config": {
        "googleSignIn": {
          "apiKey": "384183779621-gmgl7to8khpiuenov3vdqa24o945jnud.apps.googleusercontent.com",
          "certificateHash": "91:47:37:E8:D1:A2:D9:D8:A4:8D:68:50:56:A6:94:8E:BE:92:FC:A3"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
